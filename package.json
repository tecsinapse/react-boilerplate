{
  "name": "@tecsinapse/react-boilerplate",
  "version": "0.0.3",
  "main": "dist/index.cjs.js",
  "module": "build/index.js",
  "browser": "dist/index.js",
  "sideEffects": false,
  "dependencies": {
    "@material-ui/core": "3.9.0",
    "@material-ui/icons": "3.0.2",
    "@tecsinapse/ui-kit": "0.0.3",
    "apollo-cache-inmemory": "1.4.2",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "2.4.12",
    "apollo-link": "1.2.6",
    "apollo-link-context": "1.0.12",
    "apollo-link-error": "1.1.5",
    "apollo-link-http": "1.5.9",
    "axios": "^0.18.0",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.0",
    "keycloak-js": "4.4.0",
    "localforage": "^1.7.3",
    "raven-js": "^3.27.0",
    "react": "^16.7.0-alpha.2",
    "react-apollo": "2.4.0",
    "react-dom": "^16.7.0-alpha.2",
    "react-ga": "^2.5.6",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@tecsinapse/eslint-config-tecsinapse-js": "^2.0.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "docz": "^0.12.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-test-renderer": "^16.6.0",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.66.6",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "scripts": {
    "build": "rm -rf build/ && babel src/ -d build/ && rollup -c",
    "dev": "rollup -c -w",
    "docz:dev": "docz dev",
    "docz:build": "docz build -d docs/docz/ --base='/ui-kit/docz/' ",
    "test": "NODE_ENV=test jest test/"
  },
  "peerDependencies": {
    "react": ">= 16.3.x"
  },
  "files": [
    "dist",
    "build"
  ],
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js",
    "moduleDirectories": [
      "node_modules",
      "bower_components"
    ],
    "eslintConfig": {
      "extends": [
        "@tecsinapse/tecsinapse-js"
      ]
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged",
        "post-commit": "git update-index --again"
      }
    },
    "lint-staged": {
      "*.js": [
        "eslint --max-warnings=0 --fix",
        "prettier --write",
        "git add"
      ]
    }
  }
}
