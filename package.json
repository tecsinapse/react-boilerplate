{
  "name": "@tecsinapse/react-boilerplate",
  "description": "TecSinapse boilerplate for react projects",
  "version": "0.0.49",
  "browser": "dist/index.js",
  "main": "dist/index.cjs.js",
  "module": "build/index.js",
  "dependencies": {
    "@lingui/core": "^2.7.3",
    "@lingui/macro": "^2.7.3",
    "@lingui/react": "^2.7.3",
    "@material-ui/core": "3.9.1",
    "@material-ui/icons": "3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@mdi/js": "^3.0.39",
    "@mdi/react": "^1.1.0",
    "@tecsinapse/es-utils": "^0.0.96",
    "@tecsinapse/ui-kit": "^0.0.61",
    "apollo-cache-inmemory": "1.4.2",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "2.4.12",
    "apollo-link": "1.2.6",
    "apollo-link-context": "1.0.12",
    "apollo-link-error": "1.1.5",
    "apollo-link-http": "1.5.9",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.0",
    "keycloak-js": "4.4.0",
    "localforage": "^1.7.3",
    "material-ui-search-bar": "1.0.0-beta.13",
    "memoize-one": "^5.0.0",
    "raven-js": "^3.27.0",
    "react": "16.8.0",
    "react-apollo": "2.4.0",
    "react-confirm": "^0.1.18",
    "react-dom": "16.8.0",
    "react-ga": "^2.5.6",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.3.0",
    "react-text-mask": "^5.4.3",
    "react-virtualized": "^9.21.0",
    "react-window": "^1.5.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "text-mask-addons": "^3.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@tecsinapse/eslint-config-tecsinapse-js": "^2.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "docz": "^0.12.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react-test-renderer": "16.7.0-alpha.2",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.66.6",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "files": [
    "build",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf build/ && babel src/ -d build/ && rollup -c",
    "dev": "rollup -c -w",
    "docz:build": "docz build -d docs/docz/ --base='/ui-kit/docz/' ",
    "docz:dev": "docz dev",
    "test": "NODE_ENV=test jest test/"
  },
  "jest": {
    "eslintConfig": {
      "extends": [
        "@tecsinapse/tecsinapse-js"
      ]
    },
    "husky": {
      "hooks": {
        "post-commit": "git update-index --again",
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*.js": [
        "eslint --max-warnings=0 --fix",
        "git add",
        "prettier --write"
      ]
    },
    "moduleDirectories": [
      "bower_components",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "peerDependencies": {},
  "sideEffects": false
}
